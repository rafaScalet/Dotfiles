{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "green",
          "background": "transparent",
          "template": "{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "magenta",
          "background": "transparent",
          "template": "MINGW64 "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "yellow",
          "background": "transparent",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "cyan",
          "background": "transparent",
          "template": "({{ .HEAD }})",
          "properties": {
            "branch_icon": ""
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "white",
          "background": "transparent",
          "template": "$"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
