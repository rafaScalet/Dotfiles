{
  "files.associations": {
    "dot-gitalias": "properties",
    "dot-gitconfig": "properties",
    ".stowrc": "properties",
    "dot-aliases": "shellscript",
    "dot-bashrc": "shellscript",
    "dot-env": "dotenv",
    "dot-ignore": "ignore"
  },
  "explorer.compactFolders": true,
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.patterns": {
    "readme.md": ".gitignore, .stowrc, LICENSE, bootstrap.sh",
    "dot-env": "readme.md",
    "dot-ignore": "readme.md"
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false
  },
  "toggler.toggles": [
    [
      "absolute",
      "relative"
    ],
    [
      "high",
      "low"
    ],
    [
      "starship",
      "ohmyposh"
    ],
    [
      "git-bash",
      "zen-p10K"
    ]
  ]
}
